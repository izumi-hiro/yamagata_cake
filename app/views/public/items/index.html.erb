<h1>利用者</h1>
<h2>商品一覧</h2>
<table class="table">
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= item.id %></td>
        <td><%= item.name %></td>
        <td><%= item.tax_excluded_price %></td>
        <td><%= item.genre_id %></td>
        <td><%= item.sales_status %></td>
      </tr>
    <% end %>
  </tbody>
</table>